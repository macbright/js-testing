{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    "comma-dangle": ["error", "never"],
    "no-shadow": 0,
    "curly": 0,
    "no-param-reassign": 0,
    "eol-last": 0,
    "indent": [2, "tab"],
    "no-tabs": 0,
    "skipBlankLines": true,
    "allowShortCircuit": true,
    "allowTernary": true
  }
}